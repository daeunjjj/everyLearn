<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="adminMapper">
	
	<select id="selectOneAdminById" resultType="AdminVo">
		SELECT 
		    NO
		    ,PERMISSION
		    ,POSITION
		    ,ID
		    ,PWD
		    ,NICK
		    ,NAME
		    ,PHONE
		    ,ENROLL_DATE
		    ,MODIFY_DATE
		    ,QUIT_YN
		FROM ADMIN
		WHERE ID = #{id}
	</select>
	
</mapper>
